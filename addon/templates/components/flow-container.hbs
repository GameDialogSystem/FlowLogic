<svg width="1680px" height="1000px" viewBox="0 0 1680 1000" xmlns="http://www.w3.org/2000/svg" version="1.1">
  {{#each blocks.outputs as |output|}}
    {{flow-connection startX=output.x startY=output.y endX=output.input.x endY=output.input.y}}
  {{/each}}

  {{#if showReconnector}}
    {{flow-connection startX=start.x startY=start.y endX=end.x endY=end.y}}
  {{/if}}
</svg>
