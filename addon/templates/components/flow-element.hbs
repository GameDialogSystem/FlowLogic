<flow-input-container>
  {{#each model.inputs as |input|}}
    {{flow-input
        parent=model
        model=input
        acceptRerouting=(action "acceptRerouting")
        cancelReroute=(action "cancelReroute")}}
  {{/each}}
</flow-input-container>

<flow-output-container>
  {{#each model.outputs as |output|}}
    {{flow-output
        parent=model
        model=output
        reroute=(action "reroute")
        cancelReroute=(action "cancelReroute")}}
  {{/each}}
</flow-output-container>
